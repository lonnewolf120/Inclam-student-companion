<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane stylesheets="@../css-files/darktheme.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="codenamex.smc.viz.StackController">
   <center>
      <HBox>
         <AnchorPane>
            <children>
                <Pane fx:id="stackPane" layoutX="44.0" layoutY="14.0" />
               <HBox layoutX="37.0" layoutY="460.0" prefHeight="26.0" prefWidth="584.0">
                  <children>
                      <Button fx:id="pushButton" prefHeight="26.0" prefWidth="177.0" text="Push">
                        <HBox.margin>
                           <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                        </HBox.margin>
                     </Button>
                     <TextField fx:id="textField">
                        <HBox.margin>
                           <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                        </HBox.margin>
                     </TextField>
                      <Button fx:id="popButton" prefHeight="26.0" prefWidth="198.0" text="Pop">
                        <HBox.margin>
                           <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                        </HBox.margin>
                     </Button>
                  </children>
               </HBox>
               <Text layoutX="479.0" layoutY="306.0" strokeType="OUTSIDE" strokeWidth="0.0" text="MAX SIZE=10" textAlignment="CENTER" wrappingWidth="198.0">
                  <font>
                     <Font size="24.0" />
                  </font>
               </Text>
            </children>
         </AnchorPane>
      </HBox>
   </center>
   <top>
      <Pane BorderPane.alignment="CENTER">
         <children>
            <Button layoutX="14.0" layoutY="16.0" mnemonicParsing="false" onAction="#switchToTutorial">
               <graphic>
                  <ImageView fitHeight="74.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../images/Logos/back.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button layoutX="626.0" layoutY="13.0" mnemonicParsing="false" onAction="#closeButtonA">
               <graphic>
                  <ImageView fitHeight="27.0" fitWidth="101.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../images/Logos/Close.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </Pane>
   </top>
</BorderPane>
